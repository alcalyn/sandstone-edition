#!/usr/bin/php
<?php

require_once __DIR__.'/../vendor/autoload.php';

use Eole\Sandstone\Websocket\Server;
use App\WebsocketApplication;

$app = new WebsocketApplication([
    'project.root' => dirname(__DIR__),
    'debug' => true,
]);

$websocketServer = new Server($app);

$websocketServer->run();
